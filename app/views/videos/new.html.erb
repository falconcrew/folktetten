<div>
	<h1>Lägg till video</h1>
	<%= form_for @video do |f| %>
		<%= f.label "Namn" %><br>
		<%= f.text_field :name, class: "textfield" %><br>
		<%= f.label "Länk" %><br>
		<%= f.text_field :url, class: "textfield" %><br>
		<%= f.label "Beskrivning" %><br>
		<%= f.text_area :description %><br>
		<%= f.submit "Lägg till video", class: "btn-submit" %>
	<% end %>
</div>